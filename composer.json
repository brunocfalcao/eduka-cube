{
    "name": "brunocfalcao/eduka-cube",
    "description": "Eduka Cube",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/eduka-cube",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "brunocfalcao/eduka-abstracts": "dev-master",
        "brunocfalcao/laravel-helpers": "dev-master",
        "spatie/laravel-medialibrary": "^11.8",
        "illuminate/contracts": "^10.0||^11.0",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "staudenmeir/belongs-to-through":"^2.5"
    },
    "autoload": {
        "psr-4": {
            "Eduka\\Cube\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-eduka-cubes": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduka\\Cube\\CubeServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
